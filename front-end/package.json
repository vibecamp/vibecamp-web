{
  "name": "vibecamp-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "statics": "mkdir -p out/ && cp public/* out/",
    "dev": "npm run statics && sass src/index.scss out/app.css --watch & node build.js --watch & tsc --watch & npm run serve",
    "check": "eslint . && tsc",
    "fix": "eslint . --fix",
    "build": "npm run check && npm run statics && npx sass src/index.scss out/app.css --style=compressed && node build.js --minify",
    "serve": "http-server out"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.3.1",
    "@stripe/stripe-js": "^2.1.7",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/uuid": "^9.0.0",
    "dayjs": "^1.11.10",
    "debounce": "^1.2.1",
    "esbuild": "^0.18.11",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "markdown-it": "^13.0.1",
    "markdown-it-named-headings": "^1.1.0",
    "qrcode.react": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/debounce": "^1.2.1",
    "@types/markdown-it": "^12.2.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "8.31.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "http-server": "^14.1.1",
    "sass": "^1.63.6",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "24.x.x"
  }
}